{
  "env": {
    "test": {
      "presets": [
        "@babel/preset-react",
        "@babel/preset-env"
      ],
      "plugins": [
        "add-module-exports",
        "@babel/plugin-proposal-class-properties",
        "istanbul"
      ],
      "comments": false
    },
    "production": {
      "presets": [
        "@babel/preset-react",
        "@babel/preset-env"
      ],
      "plugins": [
        "add-module-exports",
        "@babel/plugin-proposal-class-properties"
      ],
      "comments": false
    }
  }
}
